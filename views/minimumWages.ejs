<body class="royal_preloader ">
    <div id="page" class="site">
        <%- include("partials/navbar") %>

        <div id="content" class="site-content">
            <div class="page-header flex-middle">
                <div class="container">
                    <div class="inner flex-middle">
                        <% if(wages){ %>
                        <h1 class="page-title">Minimum Wages for <span style="font-weight: bolder;">
                                <%= wages.state %></span> </h1>
                        <% }else{ %>
                        <h1 class="page-title">No data available </h1>
                        <% } %>
                        <ul id="breadcrumbs" class="breadcrumbs none-style">
                            <li><a href="/">Home</a></li>
                            <li class="active">Minimum Wages</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <% if(wages){ %>
             <% wages.allWages.forEach(function(wages){ %> 
                <div style="text-align: center;padding:30px">
                    <h6>Effective Date: <%= wages.effectiveDate %> </h6>
                    <a href="/minimumWagesSelector">Go Back</a>
                </div>
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                        
                            <table class="table ">
                                <thead style="background: #e6e4e4;color:black">
                                    <tr class="table-head">
                                        <th>Employment</th>
                                        <th scope="col">Category of workmen/Employees</th>
                                        <th scope="col">Minimum rates of wages in Rupees <span class="rates-wages"><span>Per
                                                    Month</span><span>Per
                                                    Day</span></span></th>
                                        <th>Notification Attachment </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for(let i=0; i<wages.category.length; i++){ %>
                                    <tr>
                                        <td style="border: none;"><%= i==1 ? wages.schedule: null %></td>
                                        <td> <%= wages.category[i] %> </td>
                                        <td class="rates-wages"><span><%= wages.minimumRates.ratesPerMonth[i] %> </span>
                                            <span><%= wages.minimumRates.ratesPerDay[i] %> </span></td>
                                        <td style="border: none;"> <a style=<%= i!=1 && "display:none"  %>  target="_blank" href="/uploads/<%-wages.filename%>"
                                            download="/uploads/<%-wages.filename%>">Download latest Govt. Notification here<i class="fas fa-file-pdf"></i></a></td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        
                        </div>
                    </div>
                </div>
            <% }) %>  
        <% }%>
    </div><!-- #page -->
